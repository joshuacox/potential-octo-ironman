auto lo
iface lo inet loopback

allow-hotplug eth0
iface eth1 inet static 
	address 66.226.76.152
	netmask 255.255.255.0
	gateway 216.55.137.1
	dns-nameservers 8.8.8.8 8.8.4.4
	pre-up sleep 2

allow-hotplug eth1
iface eth0 inet static 
	address 10.1.201.152
	netmask 255.255.255.0
	gateway 10.1.201.1
	network 10.1.201.0
	broadcast 10.1.201.255
	dns-nameservers 8.8.8.8 8.8.4.4
	pre-up sleep 2
